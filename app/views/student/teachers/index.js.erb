var teachers = document.querySelector(".user-list");
var pagination = document.querySelector(".info-pagination");

teachers.innerHTML = "";
pagination.innerHTML = "";

pagination.insertAdjacentHTML("beforeend", "<%= j render 'layouts/shared/pagination', items: @teachers %>");

<% @teachers.each do |teacher| %>
  teachers.insertAdjacentHTML("beforeend", "<%= j render 'student/teachers/shared/teacher_card', teacher: teacher %>");
<% end %>
